FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;
setFormat raw;
sets
(
    velocity
  {
    type uniform;
    axis y;
    start (0.005 -0.002 0);
    end (0.005 -0.00032128 0);
    nPoints 100;
  }
);



surfaceFormat      raw;

surfaces        
(
    constantIso
    {
        // Iso surface for constant values.
        // Triangles guaranteed not to cross cells.
        type            isoSurface;    // always triangulated
        isoField        alpha1;
        isoValue        0.5;
        interpolate     true;
        regularise      true;              // do not simplify
    }
);

fields            
( 
    U    
    alpha1 
);


// ************************************************************************* //
